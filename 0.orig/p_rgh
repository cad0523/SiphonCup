FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      p_rgh;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -1 -2 0 0 0 0];

internalField   uniform 0; // p_rgh
//internalField   uniform 101325; // p

boundaryField
{
    outlet
    {
//        type            zeroGradient;
//        type            prghPressure;
        type            fixedValue;
        p0              $internalField;
        value           $internalField;
    }

    atmosphere
    {
        type            fixedValue;
//        type            totalPressure;
        p0              $internalField;
        value           $internalField;
    }

    hull
    {
        type            fixedFluxPressure;
    }
}


